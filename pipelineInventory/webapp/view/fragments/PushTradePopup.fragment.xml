    <Dialog title="Please select a Dealer" class="sapUiPopupWithPadding">

<subHeader>

<Toolbar>

<ComboBox id="VLRDealer" selectionChange="onDealerChange" items="{BpDealerModelZone>/}" class="sapUiTinyMarginBegin" width="140%">
											<core:ListItem text="{BpDealerModelZone>BusinessPartner} - {BpDealerModelZone>BusinessPartnerName}" key="{BpDealerModelZone>BusinessPartnerKey}"/>
											<layoutData>
												<l:GridData span="L2 M2 S6"/>
											</layoutData>
										</ComboBox>

</Toolbar>

</subHeader>

<content>

<Text id="txt_routable" visible="false" text="This vehicle is routable. Do you want to proceed?" />
<Text id="txt_nonroutable" visible="false" text="This vehicle is non-routable. Do you want to proceed?" />

<Text id="txt_nontradeable" visible="false" width="300px" text="This vehicle is non-tradeable."/>
</content>

<beginButton>

<Button text="Close" press="onCloseDialog"/>

</beginButton>

<buttons>

    <Button xmlns="sap.m" text="Yes" id="btn_yes" visible="false" press="onYesClick"/>

    <Button xmlns="sap.m" text="No" id="btn_no" visible="false" press="onNoClick"/>

    <Button xmlns="sap.m" text="Close" id="btn_close" press="onCloseClick"/>

</buttons>

</Dialog>
