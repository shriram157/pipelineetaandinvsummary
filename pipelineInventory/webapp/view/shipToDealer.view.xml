<mvc:View xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core"
	xmlns:f="sap.ui.layout.form" xmlns:tnt="sap.tnt" controllerName="pipelineInventory.controller.shipToDealer" xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page showNavButton="false">
				<customHeader>
					<Bar class="sapUiSmallPaddingBeginEnd clCustomHeaderBg">
						<contentLeft>
							<Image id="idLexusLogo" src="images/Lexus.png" width="100px"/>
							<Text text="|" class="sapUiTinyMarginBeginEnd"/>
							<Text class="clCustomFont" text="{i18n>PageTitle}"/>
						</contentLeft>
						<contentRight>
							<!--<Select change="selectedScreen" selectedKey="Ship To Dealer">-->
							<!--	<items>-->
							<!--		<core:Item key="Master" text="Master"/>-->
							<!--		<core:Item key="Details" text="Details"/>-->
							<!--		<core:Item key="Vehicle Details" text="Vehicle Details"/>-->
							<!--		<core:Item key="Order Change" text="Order Change"/>-->
							<!--		<core:Item key="Ship To Dealer" text="Ship To Dealer"/>-->
							<!--		<core:Item key="Ship To Dealer Response" text="Ship To Dealer Response"/>-->
							<!--		<core:Item key="Assign Vehicles" text="Assign Vehicles"/>-->
							<!--		<core:Item key="Assign Vehicles Status" text="Assign Vehicles Status"/>-->
							<!--		<core:Item key="Change History" text="Change History"/>-->
							<!--	</items>-->
							<!--</Select>-->
							<Button text="{i18n>Logout}"/>
						</contentRight>
					</Bar>
				</customHeader>
				<tnt:ToolHeader>
					<!--<Select change="selectedScreen" selectedKey="Ship To Dealer">-->
					<!--	<items>-->
					<!--		<core:Item key="Master" text="Master"/>-->
					<!--		<core:Item key="Details" text="Details"/>-->
					<!--		<core:Item key="Vehicle Details" text="Vehicle Details"/>-->
					<!--		<core:Item key="Order Change" text="Order Change"/>-->
					<!--		<core:Item key="Ship To Dealer" text="Ship To Dealer"/>-->
					<!--		<core:Item key="Ship To Dealer Response" text="Ship To Dealer Response"/>-->
					<!--		<core:Item key="Assign Vehicles" text="Assign Vehicles"/>-->
					<!--		<core:Item key="Assign Vehicles Status" text="Assign Vehicles Status"/>-->
					<!--		<core:Item key="Change History" text="Change History"/>-->
					<!--	</items>-->
					<!--</Select>-->
					<Link text="{i18n>PageTitle}" class="clCustomFont sapUiSmallMargin" press="onMenuLinkPress"/>
					<Link text="{i18n>VehicleDetails}" class="clCustomFont sapUiSmallMargin" press="onMenuLinkPress"/>
					<Link text="{i18n>ChangeHistory}" class="clCustomFont sapUiSmallMargin" press="onMenuLinkPress"/>
				</tnt:ToolHeader>
				<content>
					<Label text="{i18n>Dropship}" design="Bold" class="styleFontSize sapUiSmallPadding sapUiSmallMargin"/>
					<OverflowToolbar>
						<HBox>
							<!--<Label text="{i18n>ShipToDealer}" design="Bold" class="styleFontSizeSmall sapUiSmallMargin" required="required"/>-->
							<!--<Select forceSelection="true" items="{SelectJSONModel>/SeriesCollection}">-->
							<!--	<core:Item key="{SelectJSONModel>Series}" text="{SelectJSONModel>SeriesName} {SelectJSONModel>Dimension}"/>-->
							<!--</Select>-->
							<HBox>
								<Label text="{i18n>ShipToDealer}" design="Bold" class="styleFontSizeSmall sapUiSmallMargin" required="required"/>
								<ComboBox showSecondaryValues="true" filterSecondaryValues="true" value="" selectedKey=""
									items="{ path: 'BusinessDataModel>/DealerList', sorter: { path: 'BusinessPartner' } }">
									<core:ListItem key="{BusinessDataModel>BusinessPartner}" text="{BusinessDataModel>BusinessPartner}"
										additionalText="{BusinessDataModel>BusinessPartnerName}"/>
								</ComboBox>
							</HBox>
						</HBox>
						<ToolbarSpacer/>
						<Button text="{i18n>Submit}" type="Accept"/>
					</OverflowToolbar>
					<Table items="{DropShipDataModel>/results}" width="auto" class="sapUiResponsiveMargin">
						<headerToolbar>
							<Toolbar>
								<MessageStrip text="{i18n>Disclaimer}" type="Information" showIcon="true"></MessageStrip>
							</Toolbar>
						</headerToolbar>
						<columns>
							<Column minScreenWidth="Desktop" demandPopin="true" hAlign="Center">
								<Text text="{i18n>OrderNumber}"/>
							</Column>
							<Column minScreenWidth="Desktop" demandPopin="true" hAlign="Center">
								<Text text="{i18n>OrderType}"/>
							</Column>
							<Column minScreenWidth="Desktop" demandPopin="true" hAlign="Center">
								<Text text="{i18n>Status}"/>
							</Column>
							<Column minScreenWidth="Desktop" demandPopin="true">
								<Text text="{i18n>VTN}"/>
							</Column>
							<Column minScreenWidth="Desktop" demandPopin="true">
								<Text text="{i18n>VIN}"/>
							</Column>
							<Column minScreenWidth="Desktop" demandPopin="true">
								<Text text="{i18n>Model}"/>
							</Column>
							<Column minScreenWidth="Desktop" demandPopin="true">
								<Text text="{i18n>Suffix}"/>
							</Column>
							<Column minScreenWidth="Desktop" demandPopin="true">
								<Text text="{i18n>Colour}"/>
							</Column>
							<Column minScreenWidth="Desktop" demandPopin="true">
								<Text text="{i18n>ETAFrom}"/>
							</Column>
							<Column minScreenWidth="Desktop" demandPopin="true">
								<Text text="{i18n>ETATo}"/>
							</Column>
						</columns>
						<items>
							<ColumnListItem>
								<cells>
									<Link text="{DropShipDataModel>ZZDLR_REF_NO}" press="onNavigateToVL" tooltip="{DropShipDataModel>ZZDLR_REF_NO}"/>
									<Text text="{DropShipDataModel>ZZORDERTYPE}"/>
									<Text text="{DropShipDataModel>ZMMSTA}"/>
									<Text text="{DropShipDataModel>ZZVTN}" tooltip="{DropShipDataModel>ZZVTN}"/>
									<Text text="{DropShipDataModel>VHVIN}" tooltip="{DropShipDataModel>VHVIN}"/>
									<Text text="{DropShipDataModel>Model}"/>
									<Text text="{DropShipDataModel>Suffix}"/>
									<Text text="{DropShipDataModel>ExteriorColorCode}"/>
									<Text text="{DropShipDataModel>ETAFrom}"/>
									<Text text="{DropShipDataModel>ETA}"/>
								</cells>
							</ColumnListItem>
						</items>
					</Table>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>