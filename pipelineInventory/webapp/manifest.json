{
	"_version": "1.8.0",
	"sap.app": {
		"id": "pipelineInventory",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "2.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZPIPELINE_ETA_INVENT_SUMMARY_SRV": {
				"uri": "/node/ZPIPELINE_ETA_INVENT_SUMMARY_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/ZPIPELINE_ETA_INVENT_SUMMARY_SRV/metadata.xml"
				}
			},
				"Z_VEHICLE_MASTER_SRV": {
				"uri": "/node/Z_VEHICLE_MASTER_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
					
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "pipelineInventory.view.master",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.ushell": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "pipelineInventory.i18n.i18n"
				},
				"preload": true
			},
			"OrderChangeModel": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request",
					"json": true,
					"useBatch": false,
					"headers": {
						"X-Requested-With": "XMLHttpRequest"
					}
				},
				"dataSource": "ZPIPELINE_ETA_INVENT_SUMMARY_SRV",
				"preload": false
			},
				"VehicleModel": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request",
					"json": true,
					"useBatch": false,
					"headers": {
						"X-Requested-With": "XMLHttpRequest"
					}
				},
				"dataSource": "Z_VEHICLE_MASTER_SRV",
				"preload": false
			},
			"DataModel": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request",
					"json": true,
					"useBatch": false,
					"headers": {
						"X-Requested-With": "XMLHttpRequest"
					}
				},
				"dataSource": "ZPIPELINE_ETA_INVENT_SUMMARY_SRV",
				"preload": false
			}
		},
		"resources": {
			"js": [
				{
				"uri": "utils/fabIcons.js"
				},
				{
				"uri": "libs/moment.js"
				},
				{
				"uri": "libs/moment-timezone.min.js"
				}
			],
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "pipelineInventory.view",
				"controlAggregation": "pages",
				"controlId": "idAppControl",
				"clearControlAggregation": false
			},
			"routes": [{
				"name": "Routemaster",
				"pattern": "Routemaster",
				"target": "Routemaster"
			}, {
				"name": "details",
				"pattern": "details/{tableFirst}",
				"target": "details"
			}, {
				"name": "vehicleDetails",
				"pattern": "vehicleDetails/{VCData}",
				"target": "vehicleDetails"
			}, {
				"name": "vehicleDetails2",
				"pattern": "vehicleDetails2/{VCData2}",
				"target": "vehicleDetails2"
			}, {
				"name": "vehicleDetailsNodata",
				"pattern": "vehicleDetailsNodata",
				"target": "vehicleDetailsNodata"
			}, {
				"name": "orderChange",
				"pattern": "orderChange/{Data}",
				"target": "orderChange"
			}, {
				"name": "orderChange2",
				"pattern": "orderChange2/{Data2}",
				"target": "orderChange2"
			}, {
				"name": "shipToDealer",
				"pattern": "shipToDealer/{vehicleData}",
				"target": "shipToDealer"
			}, {
				"name": "shipToDealerResponse",
				"pattern": "shipToDealerResponse/{data}",
				"target": "shipToDealerResponse"
			}, {
				"name": "assignVehicles",
				"pattern": "assignVehicles/{vehicleData}",
				"target": "assignVehicles"
			}, {
				"name": "assignVehiclesStatus",
				"pattern": "assignVehiclesStatus/{data}",
				"target": "assignVehiclesStatus"
			}, {
				"name": "changeHistory",
				"pattern": "changeHistory/{SelectedDealer}",
				"target": "changeHistory"
			}, {
				"name": "changeHistory2",
				"pattern": "changeHistory",
				"target": "changeHistory2"
			}],
			"targets": {
				"Routemaster": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "master",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "pipelineInventory.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl"
				},
				"details": {
					"viewType": "XML",
					"viewName": "details"
				},
				"vehicleDetails": {
					"viewType": "XML",
					"viewName": "vehicleDetails"
				},
				"vehicleDetails2": {
					"viewType": "XML",
					"viewName": "vehicleDetails"
				},
				"vehicleDetailsNodata": {
					"viewType": "XML",
					"viewName": "vehicleDetails"
				},
				"orderChange": {
					"viewType": "XML",
					"viewName": "orderChange"
				},
				"orderChange2": {
					"viewType": "XML",
					"viewName": "orderChange"
				},
				"shipToDealer": {
					"viewType": "XML",
					"viewName": "shipToDealer"
				},
				"shipToDealerResponse": {
					"viewType": "XML",
					"viewName": "shipToDealerResponse"
				},
				"assignVehicles": {
					"viewType": "XML",
					"viewName": "assignVehicles"
				},
				"assignVehiclesStatus": {
					"viewType": "XML",
					"viewName": "assignVehiclesStatus"
				},
				"changeHistory": {
					"viewType": "XML",
					"viewName": "changeHistory"
				},
				"changeHistory2": {
					"viewType": "XML",
					"viewName": "changeHistory"
				}
			}
		}
	}
}