{
	"welcomeFile": "/pipelineInventory/index.html",
	"authenticationMethod": "route",
	"logout": {
		"logoutEndpoint": "/do/logout"
	},
	"routes": [{
		"source": "^/pipelineInventory/(.*)$",
		"target": "$1",
		"localDir": "webapp",
		"authenticationType": "xsuaa"
	}, {
		"source": "/node(.*)",
		"destination": "core-node-backend",
		"csrfProtection": false,
		"authenticationType": "xsuaa"
	}, {
		"source": "/userDetails(.*)",
		"destination": "core-node-backend",
		"csrfProtection": false,
		"authenticationType": "xsuaa"
	}]
}